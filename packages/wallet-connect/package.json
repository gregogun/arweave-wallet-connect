{
  "name": "arweave-wallet-ui-test",
  "version": "0.0.15",
  "description": "An Arweave Wallet UI module that handles connecting a wallet and showing a profile modal.",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc && tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "tsc",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "version": "pnpm version",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@aura-ui/react": "0.0.5-alpha.3",
    "@radix-ui/colors": "0.1.8",
    "@radix-ui/react-icons": "1.1.1",
    "@stitches/react": "1.2.8",
    "arconnect": "0.4.2",
    "arweave-account": "1.3.4",
    "arweave-graphql": "0.0.5",
    "arweave-wallet-connector": "1.0.2",
    "boring-avatars": "1.7.0",
    "react-icons": "4.10.1",
    "vouchdao": "0.2.6"
  },
  "devDependencies": {
    "@types/node": "18.11.13",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "arlocal": "1.1.59",
    "arweave": "1.13.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "5.11.7",
    "typescript": "4.6.4"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17.0 || ^18.0",
    "react-dom": "^16.8 || ^17.0 || ^18.0"
  },
  "type": "module"
}
